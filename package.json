{
  "name": "learning-and-engagement-tool",
  "version": "1.0.0",
  "main": "index.js",
  "description": "CSC2033-Software Engineering Team Project, Team 54, EPiC Learning and Engagement Tool",
  "scripts": {
    "docker": "docker-compose --project-name epic_app -f docker_config/docker-compose.yml up",
    "rebuild": "docker-compose --project-name epic_app -f docker_config/docker-compose.yml build --force-rm",
    "backend": "docker exec -it web npm run start",
    "install": "docker exec web npm i",
    "killall": "docker exec -it web npm run stop-all",
    "frontend": "docker exec -it web npm run webpack",

    "frontend:livereload": "docker exec -it web npm run livereload",
    "stop-all": "killall node",
    "start": "nodemon -L --watch ./backend --exec babel-node backend/app.js",
    "webpack": "webpack --watch",
    "livereload": "npm run webpack & browser-sync start --proxy 'localhost:3000' --no-open --files 'dist'"
  },
  "author": "Erikas Nakonecnikovas, Danila Grobov, Jake Hobbs, Max Champagne, Sofia Trevino",
  "license": "ISC",
  "dependencies": {
    "react-router": "^5.2.0",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@mysql/xdevapi": "^8.0.22",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "crypto-random-string": "^3.3.0",
    "css-loader": "^5.0.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "node-sass": "^5.0.0",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-toastify": "^6.2.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0"
  }
}
